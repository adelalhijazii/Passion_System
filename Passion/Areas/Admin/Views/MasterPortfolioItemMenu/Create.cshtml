@model Passion.Areas.Admin.ViewModels.MasterPortfolioItemMenuViewmodel

@{
    ViewData["Title"] = "Create";
}

<div class="row mb-3">
    <div class="col-12">
        <div class="text-start">
            <h1 class="fw-bolder">New PortfolioItemMenu</h1>
        </div>
    </div>
</div>

<form asp-action="Create" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input asp-for="MasterPortfolioItemMenuId" class="form-control" type="hidden" value="0" />
    <div class="row m-0">
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioCategoryMenu.MasterPortfolioCategoryMenuName" class="form-label"></label>
                <select asp-for="MasterPortfolioCategoryMenuId"
                        asp-items="@(new SelectList(ViewBag.category, "MasterPortfolioCategoryMenuId", "MasterPortfolioCategoryMenuName"))" class="form-control form-select">
                </select>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuTitle" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuTitle" class="form-control" placeholder="Enter Title" />
                <span asp-validation-for="MasterPortfolioItemMenuTitle" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuBreef" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuBreef" class="form-control" placeholder="Enter Breef" />
                <span asp-validation-for="MasterPortfolioItemMenuBreef" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuFile" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuFile" type="file" class="form-control" />
                <span asp-validation-for="MasterPortfolioItemMenuFile" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsTitle" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuDetailsTitle" class="form-control" placeholder="Enter DetailsTitle" />
                <span asp-validation-for="MasterPortfolioItemMenuDetailsTitle" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsDate" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuDetailsDate" class="form-control" placeholder="Enter Date" />
                <span asp-validation-for="MasterPortfolioItemMenuDetailsDate" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsCompany" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuDetailsCompany" class="form-control" placeholder="Enter Company" />
                <span asp-validation-for="MasterPortfolioItemMenuDetailsCompany" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsUrlCompany" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuDetailsUrlCompany" class="form-control" placeholder="Enter UrlCompany" />
                <span asp-validation-for="MasterPortfolioItemMenuDetailsUrlCompany" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsProjectFeatures" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuDetailsProjectFeatures" class="form-control" placeholder="Enter ProjectFeatures" />
                <span asp-validation-for="MasterPortfolioItemMenuDetailsProjectFeatures" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsTechnologiesUsed" class="form-label"></label>
                <textarea asp-for="MasterPortfolioItemMenuDetailsTechnologiesUsed" class="form-control" rows="4" placeholder="Enter TechnologiesUsed"></textarea>
                <span asp-validation-for="MasterPortfolioItemMenuDetailsTechnologiesUsed" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsDescription1" class="form-label"></label>
                <textarea asp-for="MasterPortfolioItemMenuDetailsDescription1" class="form-control" rows="4" placeholder="Enter Description"></textarea>
                <span asp-validation-for="MasterPortfolioItemMenuDetailsDescription1" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsDescription2" class="form-label"></label>
                <textarea asp-for="MasterPortfolioItemMenuDetailsDescription2" class="form-control" rows="4" placeholder="Enter Description"></textarea>
                <span asp-validation-for="MasterPortfolioItemMenuDetailsDescription2" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsActiveUsers" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuDetailsActiveUsers" class="form-control" placeholder="Enter ActiveUsers" />
