        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsFeatures2" class="form-label"></label>
                <textarea asp-for="MasterPortfolioItemMenuDetailsFeatures2" class="form-control" rows="4" placeholder="Enter Feature"></textarea>
                <span asp-validation-for="MasterPortfolioItemMenuDetailsFeatures2" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsFeatures3" class="form-label"></label>
                <textarea asp-for="MasterPortfolioItemMenuDetailsFeatures3" class="form-control" rows="4" placeholder="Enter Feature"></textarea>
                <span asp-validation-for="MasterPortfolioItemMenuDetailsFeatures3" class="text-danger"></span>
            </div>
        </div>
    </div>
    <div class="text-end m-3">
        <input type="submit" value="Create" class="btn btn-primary" />
        <a asp-action="Index" class="btn btn-success">Back to List</a>
    </div>
</form>
