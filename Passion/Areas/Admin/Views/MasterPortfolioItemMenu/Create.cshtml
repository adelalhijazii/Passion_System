        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsClientSatisfaction" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuDetailsClientSatisfaction" class="form-control" placeholder="Enter ClientSatisfaction" />
                <span asp-validation-for="MasterPortfolioItemMenuDetailsClientSatisfaction" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsMonthsDevelopment" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuDetailsMonthsDevelopment" class="form-control" placeholder="Enter MonthsDevelopment" />
                <span asp-validation-for="MasterPortfolioItemMenuDetailsMonthsDevelopment" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsAppStoreRating" class="form-label"></label>
                <input asp-for="MasterPortfolioItemMenuDetailsAppStoreRating" class="form-control" placeholder="Enter AppStoreRating" />
                <span asp-validation-for="MasterPortfolioItemMenuDetailsAppStoreRating" class="text-danger"></span>
            </div>
        </div>
        <div class="col-8 mb-3"></div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsTheChallenge" class="form-label"></label>
                <textarea asp-for="MasterPortfolioItemMenuDetailsTheChallenge" class="form-control" rows="4" placeholder="Enter Challenge"></textarea>
                <span asp-validation-for="MasterPortfolioItemMenuDetailsTheChallenge" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsTheSolution" class="form-label"></label>
                <textarea asp-for="MasterPortfolioItemMenuDetailsTheSolution" class="form-control" rows="4" placeholder="Enter Solution"></textarea>
                <span asp-validation-for="MasterPortfolioItemMenuDetailsTheSolution" class="text-danger"></span>
            </div>
        </div>
        <div class="col-4 mb-3"></div>
        <div class="col-4 mb-3">
            <div class="form-group">
                <label asp-for="MasterPortfolioItemMenuDetailsFeatures1" class="form-label"></label>
                <textarea asp-for="MasterPortfolioItemMenuDetailsFeatures1" class="form-control" rows="4" placeholder="Enter Feature"></textarea>
                <span asp-validation-for="MasterPortfolioItemMenuDetailsFeatures1" class="text-danger"></span>
