@model Passion.ViewModels.HomeViewModel

@{
    ViewData["Title"] = "Home Page";
}

<!-- Hero Section -->
<section id="hero" class="hero section dark-background">

    <div class="hero-background">
        <img src="~/Pictures/SystemSetting/@Model.Setting.SystemSettingWelcomeNoteImageUrl" alt="" data-aos-duration="1000">
        <div class="overlay"></div>
    </div>

    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6">
                <div class="hero-content">
                    <span class="hero-badge">@Model.Setting.SystemSettingWelcomeNoteTitle</span>
                    <h1>@Model.Setting.SystemSettingWelcomeNoteBreef</h1>
                    <p>@Model.Setting.SystemSettingWelcomeNoteDescription</p>
                    <div class="hero-actions">
                        @Html.Raw(Model.Setting.SystemSettingServicesUrl)
                    </div>
                    <div class="hero-stats">
                        <div class="stat-item">
                            @Html.Raw(Model.Setting.SystemSettingProjectsCompleted)
                        </div>
                        <div class="stat-item">
                            @Html.Raw(Model.Setting.SystemSettingClientSatisfaction)
                        </div>
                        <div class="stat-item">
                            @Html.Raw(Model.Setting.SystemSettingSupportAvailable)
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="hero-visual">
                    <div class="row g-3">
                        <div class="col-6">
                            <div class="feature-card">
                                @Html.Raw(Model.Setting.SystemSettingSecureAndReliable)
                            </div>
                            <div class="feature-card">
                                @Html.Raw(Model.Setting.SystemSettingExpertTeam)
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="feature-card">
                                @Html.Raw(Model.Setting.SystemSettingHighPerformance)
                            </div>
                            <div class="feature-card">
                                @Html.Raw(Model.Setting.SystemSettingAwardWinning)
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>

<!-- About Section -->
<section id="about" class="about section">

    <div class="container">

        <div class="row align-items-center">
            <div class="col-lg-6">
                <div class="content">
                    <h2>@Model.Setting.SystemSettingAboutTitle</h2>
                    <p class="lead">@Model.Setting.SystemSettingAboutBreef</p>
                    <p>@Model.Setting.SystemSettingAboutDescription1</p>
                    <p>@Model.Setting.SystemSettingAboutDescription2</p>

                    <div class="stats-container">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="stat-item">
                                    @Html.Raw(Model.Setting.SystemSettingAboutYearsExperience)
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="stat-item">
                                    @Html.Raw(Model.Setting.SystemSettingAboutProjectsCompleted)
                                </div>
                            </div>
                            <div class="col-md-4">
                                <div class="stat-item">
                                    @Html.Raw(Model.Setting.SystemSettingAboutTeamMembers)
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="cta-wrapper">
                        @Html.Raw(Model.Setting.SystemSettingAboutPortfolioUrl)
                        @Html.Raw(Model.Setting.SystemSettingAboutTeamUrl)
                    </div>
                </div>
            </div>

            <div class="col-lg-6">
                <div class="image-wrapper">
                    <img src="~/Pictures/SystemSetting/@Model.Setting.SystemSettingAboutImageUrl" alt="About Us" class="img-fluid main-image">
                    <div class="floating-card">
                        <div class="card-content">
                            @Html.Raw(Model.Setting.SystemSettingAboutAwardIcon)
                            <div class="text">
                                @Html.Raw(Model.Setting.SystemSettingAboutAwardTitle)
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

</section>

<!-- Clients Section -->
<section id="clients" class="clients section">
    <div>
        <div class="clients-slider swiper init-swiper">
            <script type="application/json" class="swiper-config">
                {
                  "loop": true,
                  "speed": 6000,
                  "autoplay": {
                    "delay": 1,
                    "disableOnInteraction": false
                  },
                  "centeredSlides": true,
                  "slideToClickedSlide": true,
                  "slidesPerView": "auto",
                  "spaceBetween": 40,
                  "breakpoints": {
                    "320": {
                      "slidesPerView": 2,
                      "spaceBetween": 20
                    },
                    "640": {
                      "slidesPerView": 3,
                      "spaceBetween": 20
                    },
                    "992": {
                      "slidesPerView": 4,
                      "spaceBetween": 30
                    },
                    "1200": {
                      "slidesPerView": 6,
                      "spaceBetween": 40
                    }
                  }
                }
            </script>
            <div class="swiper-wrapper">
                @foreach (var item in Model.ListPartner)
                {
                    <div class="swiper-slide">
                        <div class="client-logo">
                            <img src="~/Pictures/MasterPartner/@item.MasterPartnerLogoImageUrl" class="img-fluid" alt="">
                        </div>
                    </div>
                }
                @foreach (var item in Model.ListPartner)
                {
                    <div class="swiper-slide">
                        <div class="client-logo">
                            <img src="~/Pictures/MasterPartner/@item.MasterPartnerLogoImageUrl" class="img-fluid" alt="">
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</section>

<!-- Featured Services Section -->
<section id="featured-services" class="featured-services section light-background">
    <div class="container section-title">
        <h2>Featured Services</h2>
        <p>Featured Srvices</p>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="services-content" data-aos-duration="900">
                    <span class="subtitle">@Model.Feature.MasterFeatureTitle</span>
                    <h2>@Model.Feature.MasterFeatureBreef</h2>
                    <p data-aos-duration="800">@Model.Feature.MasterFeatureDescription</p>
                    <div class="mt-4" data-aos-duration="1100">
                        @Html.Raw(Model.Feature.MasterFeatureUrl)
                    </div>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="services-image">
                    <img src="~/Pictures/MasterFeature/@Model.Feature.MasterFeatureImageUrl" alt="Business Services" class="img-fluid">
                    <div class="shape-circle"></div>
                    <div class="shape-accent"></div>
                </div>
            </div>
        </div>
        <div class="row mt-5" data-aos-duration="1000">
            <div class="col-12">
                <div class="services-slider swiper init-swiper">
                    <script type="application/json" class="swiper-config">
                        {
                          "slidesPerView": 3,
                          "spaceBetween": 20,
                          "loop": true,
                          "speed": 600,
                          "autoplay": {
                            "delay": 5000
                          },
                          "navigation": {
                            "nextEl": ".swiper-nav-next",
                            "prevEl": ".swiper-nav-prev"
                          },
                          "breakpoints": {
                            "320": {
                              "slidesPerView": 1
                            },
                            "768": {
                              "slidesPerView": 2
                            },
                            "992": {
                              "slidesPerView": 3
                            }
                          }
                        }
                    </script>
                    <div class="swiper-wrapper">
                        @foreach (var item in Model.ListFeatures)
                        {
                            <div class="swiper-slide">
                                <div class="service-card">
                                    <div class="icon-box">
                                        @Html.Raw(item.MasterFeaturesIcon)
                                    </div>
                                    <a href="#" class="arrow-link"><i class="bi bi-arrow-right"></i></a>
                                    <div class="content">
                                        <h4><a href="#">@item.MasterFeaturesTitle</a></h4>
                                        <p>@item.MasterFeaturesBreef</p>
                                        <div class="service-number">@item.MasterFeaturesNumber</div>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="swiper-navigation">
                    <button class="swiper-nav-prev"><i class="bi bi-chevron-left"></i></button>
                    <button class="swiper-nav-next"><i class="bi bi-chevron-right"></i></button>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- How We Work Section -->
<section id="how-we-work" class="how-we-work section">
    <div class="container section-title">
        <h2>How We Work</h2>
        <p>Necessitatibus eius consequatur ex aliquid fuga eum quidem sint consectetur velit</p>
    </div>
    <div class="container">
        <div class="steps-grid">
            @foreach (var item in Model.ListHowWeWork)
            {
                <div class="step-card">
                    <div class="step-icon">
                        @Html.Raw(item.MasterHowWeWorkIcon)
                    </div>
                    <div class="step-number">@item.MasterHowWeWorkTitle</div>
                    <h3>@item.MasterHowWeWorkBreef</h3>
                    @Html.Raw(item.MasterHowWeWorkDescription)
                </div>
            }
        </div>
    </div>
</section>

<!-- Features Section -->
<section id="features" class="features section">
    <div class="container">
        <div class="row g-4">
            <div class="col-lg-4">
                <ul class="nav nav-tabs flex-column" role="tablist">
                    @for (int i = 0; i < Model.ListChooseCategory.Count; i++)
                    {
                        var category = Model.ListChooseCategory[i];
                        string tabId = $"features-tab-{i + 1}";
                        <li class="nav-item">
                            <a class="nav-link @(i == 0 ? "active" : "")" data-bs-toggle="tab" data-bs-target="#@tabId" role="tab" aria-controls="@tabId" aria-selected="@(i == 0 ? "true" : "false")">
                                <div class="d-flex align-items-center">
                                    <div class="icon-box">
                                        @Html.Raw(category.MasterChooseCategoryIcon)
                                    </div>
                                    <div class="ms-3">
                                        <h4>@category.MasterChooseCategoryTitle</h4>
                                        <p>@category.MasterChooseCategoryBreef</p>
                                    </div>
                                </div>
                            </a>
                        </li>
                    }
                </ul>
            </div>
            <div class="col-lg-8">
                <div class="tab-content">
                    @for (int i = 0; i < Model.ListChooseItemMenu.Count; i++)
                    {
                        var item = Model.ListChooseItemMenu[i];
                        string tabId = $"features-tab-{i + 1}";
                        <div class="tab-pane fade @(i == 0 ? "active show" : "")" id="@tabId" role="tabpanel">
                            <div class="content-box">
                                <div class="row g-4">
                                    <div class="col-lg-6">
                                        <h3>@item.MasterChooseItemMenuTitle</h3>
                                        <p>@item.MasterChooseItemMenuBreef</p>
                                        <p class="highlight">@item.MasterChooseItemMenuDescription</p>
                                        @Html.Raw(item.MasterChooseItemMenuList)
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="image-box">
                                            <img src="~/Pictures/MasterChooseItemMenu/@item.MasterChooseItemMenuImageUrl" alt="@item.MasterChooseItemMenuTitle" class="img-fluid" loading="lazy">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>

