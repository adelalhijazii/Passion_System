<!-- Services Section -->
<section id="services" class="services section light-background">
    <div class="container section-title">
        <h2>Services</h2>
        <p>CHECK OUR SERVICES</p>
    </div>
    <div class="container">
        <div class="row gy-4">
            @foreach (var item in Model.ListServices)
            {
                var activeClass = item.MasterServicesId == 1 ? "service-card featured" : "service-card compact";
                var columnClass = item.MasterServicesId >= 3 ? "col-lg-4 col-md-6" : "col-lg-6 col-md-6";
                <div class="@columnClass">
                    <div class="@activeClass">
                        <div class="service-icon">
                            @Html.Raw(item.MasterServicesIcon)
                        </div>
                        <div class="service-content">
                            <h3><a href="@Url.Action("ServiceDetails", new { idDetails = item.MasterServicesId })">@item.MasterServicesTitle</a></h3>
                            @Html.Raw(item.MasterServicesDescription)
                            <a href="@Url.Action("ServiceDetails", new { idDetails = item.MasterServicesId })" class="btn-cta">
                                @Html.Raw(item.MasterServicesUrl)
                            </a>
                        </div>
                        <div class="service-bg"></div>
                    </div>
                </div>
            }
        </div>
        <div class="stats-highlight">
            <div class="row">
                <div class="col-lg-3 col-md-6">
                    <div class="stat-item">
                        @Html.Raw(Model.Service.MasterServiceProjectsCompleted)
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="stat-item">
                        @Html.Raw(Model.Service.MasterServiceClientSatisfaction)
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="stat-item">
                        @Html.Raw(Model.Service.MasterServiceSupportAvailable)
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="stat-item">
                        @Html.Raw(Model.Service.MasterServiceYearsExperience)
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Pricing Section -->
<section id="pricing" class="pricing section">
    <div class="container section-title">
        <h2>Pricing</h2>
        <p>Necessitatibus eius consequatur ex aliquid fuga eum quidem sint consectetur velit</p>
    </div>
    <div class="container">
        <div class="row gy-4 justify-content-center">
            @foreach (var item in Model.ListPricing)
            {
                var activeClass = item.MasterPricingId == 2 ? "pricing-item featured" : "pricing-item";
                <div class="col-lg-4 col-md-6">
                    <div class="@activeClass">
                        <div class="pricing-icon">
                            @Html.Raw(item.MasterPricingIcon)
                        </div>
                        @Html.Raw(item.MasterPricingTitle)
                        <div class="price">
                            @Html.Raw(item.MasterPricingBreef)
                        </div>
                        @Html.Raw(item.MasterPricingDescription)
                        @Html.Raw(item.MasterPricingList)
                        @Html.Raw(item.MasterPricingUrl)
                    </div>
                </div>
            }
        </div>
    </div>
</section>

<!-- Faq Section -->
<section id="faq" class="faq section">
    <div class="container section-title">
        <h2>Frequently Asked Questions</h2>
        <p>Necessitatibus eius consequatur ex aliquid fuga eum quidem sint consectetur velit</p>
    </div>
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-10">
                <div class="faq-wrapper">
                    @foreach (var item in Model.ListQuestions)
                    {
                        var activeClass = item.MasterQuestionsId == 1 ? "faq-item faq-active" : "faq-item";
                        <div class="@activeClass">
                            <div class="faq-header">
                                <div class="faq-icon">
                                    @Html.Raw(item.MasterQuestionsIcon)
                                </div>
                                @Html.Raw(item.MasterQuestionsTitle)
                            </div>
                            <div class="faq-content">
                                <div class="content-inner">
                                    @Html.Raw(item.MasterQuestionsDescription)
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Call To Action Section -->
<section id="call-to-action" class="call-to-action section light-background">
    <div class="container">
        <div class="cta-wrapper">
            <div class="cta-shapes">
                <div class="shape shape-1"></div>
                <div class="shape shape-2"></div>
                <div class="shape shape-3"></div>
            </div>
            <div class="row g-0">
                <div class="col-lg-7">
                    <div class="cta-content p-5">
                        <span class="badge-custom">@Model.Focus.MasterFocusTitle</span>
                        <h2 class="mt-4 mb-4">@Model.Focus.MasterFocusBreef</h2>
                        <p class="mb-4">@Model.Focus.MasterFocusDescription</p>
                        <div class="row benefits-row mb-5">
                            <div class="col-md-6">
                                <div class="benefit-item">
                                    <div class="icon-box">
                                        @Html.Raw(Model.Focus.MasterFocusQuickSetupIcon)
                                    </div>
                                    <div class="benefit-content">
                                        @Html.Raw(Model.Focus.MasterFocusQuickSetupDescription)
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="benefit-item">
                                    <div class="icon-box">
                                        @Html.Raw(Model.Focus.MasterFocusFullSecurityIcon)
                                    </div>
                                    <div class="benefit-content">
                                        @Html.Raw(Model.Focus.MasterFocusFullSecurityDescription)
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="action-buttons">
                            @Html.Raw(Model.Focus.MasterFocusUrl)
                            <div class="guarantee-badge">
                                @Html.Raw(Model.Focus.MasterFocusMoneyBack)
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-5">
                    <div class="cta-image-container">
                        <img src="~/Pictures/MasterFocus/@Model.Focus.MasterFocusImageUrl" alt="Illustration" class="img-fluid main-image">
                        <div class="floating-element element-1">
                            @Html.Raw(Model.Focus.MasterFocusRatingIcon)
                            @Html.Raw(Model.Focus.MasterFocusRating)
                        </div>
                        <div class="floating-element element-2">
                            @Html.Raw(Model.Focus.MasterFocusUsersIcon)
                            @Html.Raw(Model.Focus.MasterFocusUsers)
                        </div>
                        <div class="pattern-dots"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Testimonials Section -->
<section id="testimonials" class="testimonials section">
    <div class="container">
        <div class="swiper init-swiper">
            <script type="application/json" class="swiper-config">
                {
                  "loop": true,
                  "speed": 600,
                  "autoplay": {
                    "delay": 5000
                  },
                  "slidesPerView": "auto",
                  "pagination": {
                    "el": ".swiper-pagination",
                    "type": "bullets",
                    "clickable": true
                  },
                  "breakpoints": {
                    "320": {
                      "slidesPerView": 1,
                      "spaceBetween": 40
                    },
                    "1200": {
                      "slidesPerView": 3,
                      "spaceBetween": 1
                    }
                  }
                }
            </script>
            <div class="swiper-wrapper">
                @foreach (var item in Model.ListWhatPeopleSay)
                {
                    <div class="swiper-slide">
                        <div class="testimonial-item">
                            @Html.Raw(item.MasterWhatPeopleSayDescription)
                            <img src="~/Pictures/MasterWhatPeopleSay/@item.MasterWhatPeopleSayImageUrl" class="testimonial-img" alt="">
                            <h3>@item.MasterWhatPeopleSayTitle</h3>
                            <h4>@item.MasterWhatPeopleSayBreef</h4>
                        </div>
                    </div>
                }
            </div>
            <div class="swiper-pagination"></div>
        </div>
    </div>
</section>

<!-- Portfolio Section -->
<section id="portfolio" class="portfolio section">
    <div class="container section-title">
        <h2>Portfolio</h2>
        <p>Necessitatibus eius consequatur ex aliquid fuga eum quidem sint consectetur velit</p>
    </div>
    <div class="container">
        <div class="isotope-layout" data-default-filter="*" data-layout="masonry" data-sort="original-order">
            @if (Model.ListPortfolioCategoryMenu.Count != 0 && Model.ListPortfolioItemMenu.Count != 0)
            {
                <div class="row">
                    <div class="col-lg-12 d-flex justify-content-center mt-4">
                        <ul class="portfolio-filters isotope-filters">
                            <li data-filter="*" class="filter-active">All</li>
                            @foreach (var category in Model.ListPortfolioCategoryMenu)
                            {
                                <li data-filter=".filter-@category.MasterPortfolioCategoryMenuId">
                                    @category.MasterPortfolioCategoryMenuName
                                </li>
                            }
                        </ul>
                    </div>
                </div>
                <div class="row gy-4 portfolio-container isotope-container">
                    @foreach (var item in Model.ListPortfolioItemMenu)
                    {
                        <div class="col-lg-4 col-md-6 portfolio-item isotope-item filter-@item.MasterPortfolioCategoryMenuId">
                            <div class="portfolio-wrap">
                                <img src="~/Pictures/MasterPortfolioItemMenu/@item.MasterPortfolioItemMenuImageUrl" class="img-fluid" alt="Portfolio Image" loading="lazy">
                                <div class="portfolio-info">
                                    <h4>@item.MasterPortfolioItemMenuTitle</h4>
                                    <p>@item.MasterPortfolioItemMenuBreef</p>
                                    <div class="portfolio-links">
                                        <a href="~/Pictures/MasterPortfolioItemMenu/@item.MasterPortfolioItemMenuImageUrl" class="glightbox" title="Capturing Moments"><i class="bi bi-zoom-in"></i></a>
                                        <a href="@Url.Action("PortfolioDetails", new { idDetails = item.MasterPortfolioItemMenuId })" title="More Details"><i class="bi bi-link-45deg"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            }
        </div>
    </div>
</section>

<!-- Team Section -->
<section id="team" class="team section">
    <div class="container section-title">
        <h2>Team</h2>
        <p>Necessitatibus eius consequatur ex aliquid fuga eum quidem sint consectetur velit</p>
    </div>
    <div class="container">
        <div class="team-slider swiper init-swiper">
            <script type="application/json" class="swiper-config">
                {
                  "loop": true,
                  "speed": 800,
                  "autoplay": {
                    "delay": 5000
                  },
                  "slidesPerView": 1,
                  "spaceBetween": 30,
                  "pagination": {
                    "el": ".swiper-pagination",
                    "type": "bullets",
                    "clickable": true
