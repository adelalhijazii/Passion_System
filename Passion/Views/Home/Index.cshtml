                      "slidesPerView": 2
                    },
                    "992": {
                      "slidesPerView": 3
                    },
                    "1200": {
                      "slidesPerView": 4
                    }
                  }
                }
            </script>
            <div class="swiper-wrapper">
                @foreach (var item in Model.ListTeam)
                {
                    <div class="swiper-slide">
                        <div class="team-card">
                            <div class="team-image">
                                <img src="~/Pictures/MasterTeam/@item.MasterTeamImageUrl" class="img-fluid" alt="" loading="lazy">
